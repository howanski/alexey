{
  "expo": {
    "name": "Alexey Companion",
    "slug": "alexey-companion",
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "alexey",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#434c5e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#434C5E"
      },
      "package": "howanski.alexey.companion"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
